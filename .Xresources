!Xft.dpi:120.4
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb
!Xft.font: Microsoft Yahei
!Emacs.font: Envy Code R-11
Emacs.font: Monaco-10
!Emacs.font: Consolas-11
Emacs.useXIM: true 

Xcursor.theme: moblin-cursor-theme
!Xcursor.theme: ComixCursors-opaque-White-Small
*metaSendsEscape: true

xterm*locale:true
xterm*utf8:true
xterm*utf8Title:true
xterm*fontMenu*fontdefault*Label:Default
xterm*faceName:Monaco:antialias=true:pixelsize=10
xterm*faceNameDoublesize:Droid Sans Fallback Pro:antialias=true:pixelsize=10
xterm*xftAntialias:true
xterm*cjkWidth:false
xterm*preeditType:Root


!!Urxvt set
! scroll set 
URxvt.scrollBar: False
!URxvt.scrollWithBuffer: false
URxvt.saveLines: 10000

! font set
URxvt.font:xft:Monaco:size=10:antialias=true,xft:LTH:size:9:antialias=true

! Input Method set 
URxvt.inputMethod: fcitx
!URxvt.imFont: 
URxvt.preeditType: OverTheSpot
URxvt.thickness: 0

! window geometry set 
!URxvt.geometry: 80x28+40+40
URxvt.title: Rxvt-Unicode
URxvt.tintColor:lightgray
!URxvt.shading:30
!URxvt.inheritPixmap:true

! Perl Script set
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1

!URxvt.background: rgba:0000/0000/0000/cccc
!URxvt.foreground: white 
URxvt.depth: 32
urxvt*foreground: #ffffff
URxvt.background: rgba:0000/0000/0000/cccc
!urxvt*background: #101010
urxvt*color0: #000000
urxvt*color1: #cc0000
urxvt*color2: #4e9a06
urxvt*color3: #c4a000
urxvt*color4: #3465a4
urxvt*color5: #75507b
urxvt*color6: #06989a
urxvt*color7: #d3d7cf
urxvt*color8: #555753
urxvt*color9: #ef2929
urxvt*color10: #8ae234
urxvt*color11: #fce94f
urxvt*color12: #729fcf
urxvt*color13: #ad7fa8
urxvt*color14: #34e2e2
urxvt*color15: #eeeeec
!urxvt*transparent: true
!urxvt*shading: 50
!urxvt*fading: 50